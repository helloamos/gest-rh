<% @services.each do |service| %>
      <tr>
      <td><%= service.id %></td>
        <td><%= service.nom %></td>
        <td><%= service.telephone %></td>
       
        <td class="action-buttons">
                <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_service_path(service), title: "Modifier", remote: true %>

<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), delete_service_path(service), remote: true, title: "Supprimer", class: "delete" %>
            </td>

      </tr>
    <% end %>