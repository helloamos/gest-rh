$("ul.errors").html("");
<% if @sanction.errors.any? %>
  <% @sanction.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".sanctions").html("<%= escape_javascript(render 'index') %>");
  $("#sanction-modal").modal("hide");
 
<% end %>