$("ul.errors").html("");
<% if @conge.errors.any? %>
  <% @conge.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".conges").html("<%= escape_javascript(render 'index') %>");
  $("#conge-modal").modal("hide");
 
<% end %>